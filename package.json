{
  "name": "github-action-download-maven-artifact",
  "version": "1.0.0",
  "description": "Download Maven Artifacts in GitHub action",
  "repository": {
    "type": "git",
    "url": "https://github.com/clausnz/github-action-download-maven-artifact"
  },
  "main": "src/index.js",
  "scripts": {
    "babel": "rm -rf babel && babel src -d babel",
    "ncc": "npm run babel && rm -rf dist && ncc build babel/index.js -mo dist",
    "clean": "rm -rf babel",
    "lint": "eslint --fix src/**/*.js",
    "build": "npm run lint && npm run ncc && npm run clean",
    "local": "npm run build && node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
