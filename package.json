    "base-64": "^1.0.0",
    "fs": "0.0.1-security",
    "graphql": "^15.4.0",
    "http": "0.0.1-security",
    "https": "^1.0.0",
    "mvn-artifact-download": "^5.1.0",
    "mvn-dl": "^5.1.0",
    "node-fetch": "^2.6.1",
